package com.ankur.it;

abstract class I1{
	public abstract void m1();
}

public class CoreAnkur{
	
	public static void main(String[] args) {
		//using Anonymous Inner class
		
		I1 i =  new I1(){
			@Override
			public void m1() {
				System.out.println("Hello Ankur m1()----");
			}
		};
		i.m1();
		
	}


}
